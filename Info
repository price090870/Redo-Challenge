
name = input ("Enter name here:")
address = input ("Enter address here:")
city = input ("Enter city here:")
state = input ("Enter state here:")
age = input ("Enter age here:")
int_age = int(age)
if int_age < 21:
  print ("Too young to vote")
else:
  print("Register to vote")
print("Hello " + name + "! You are " + age)

print( "Name: " + name)
print("Address: " +address)
print ("City: " + city)
print ("State: " + state)
print("Age: " + age)
